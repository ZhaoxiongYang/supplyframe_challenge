<!DOCTYPE html>
<html>

<head lang="en">
    <meta charset="UTF-8">
    <% include ../style/style.ejs%>
    <% include ../comment/head.ejs %>

</head>

<body style="background:rgb(189, 188, 188); color:rgb(7, 7, 7)">
    <%- include('../comment/top_bar.ejs',{project:apiData}); %>
    <div class="mdc-layout-grid">
        <div class="mdc-layout-grid__inner ">
            <%if(apiData.image_url){%>
            <div
                class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6-desktop mdc-layout-grid__cell--span-3-tablet mdc-layout-grid__cell--span-2-phone">
                <img style="width : 100%" src="<%=apiData.image_url%>"></div>
            <%}else{%>
            <div
                class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6-desktop mdc-layout-grid__cell--span-3-tablet mdc-layout-grid__cell--span-2-phone">
                <img style="width : 100%" src="https://cdn.hackaday.io/images/2754671430848813673.jpg"></div>
            <%}%>
              <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6 mdc-layout-grid__cell--span-3-tablet mdc-layout-grid__cell--span-2-phone">
                    <%- include('../utils/detail_list.ejs',{apiData:apiData}); %>
        </div>
        <br />
        <br />
    </div>
    <div>
        <p class="text-center">
            <span><a href="javascript:history.go(-1)"><button
                        class="mdc-button mdc-button--unelevated">BACK</button></a></span>
        </p>
    </div>
    <%- include('../utils/getNewpage.ejs',{lastpage:apiData.last_page}); %>
</body>

</html>